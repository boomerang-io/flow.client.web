{
  "name": "boomerang.app.flow",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://useboomerang.io",
  "scripts": {
    "analyze": "source-map-explorer server/build/static/js/main.*",
    "build": "react-scripts build",
    "build:rewriteAssetPaths": "cross-env APP_ROOT=/local node server/scripts/rewriteAssetPaths.js",
    "postbuild": "ncp build server/build",
    "cm": "git cz",
    "cypress": "cypress open",
    "docker": "cross-env NODE_ENV=test run-s docker:build docker:run",
    "docker:build": "docker build -t webapp .",
    "docker:run": "docker run -i -t -p 3000:3000 -e APP_ROOT=/app -e CORE_SERVICE_ENV_URL=http://localhost:8000 -e APP_ROOT=/local webapp",
    "eject": "react-scripts eject",
    "lint": "eslint -o lint-report.json ./src",
    "lint:staged": "eslint ./src",
    "start": "react-scripts start",
    "start:pf": "cross-env REACT_APP_PORT_FORWARD=true react-scripts start",
    "prod": "run-s prod:rewriteAssetPaths prod:serve",
    "prod:serve": "npm run-script dev --prefix server",
    "test": "react-scripts test --coverage --env=jsdom-fourteen",
    "test:ci-cypress": "start-server-and-test start http://localhost:3000 test:cypress",
    "test:cypress": "$(npm bin)/cypress run --config video=false",
    "test:staged": "cross-env CI=true react-scripts test --passWithNoTests --findRelatedTests --bail --env=jsdom-fourteen",
    "upgradePackage": "react-scripts upgradePackage"
  },
  "dependencies": {
    "@boomerang-io/carbon-addons-boomerang-react": "1.1.5",
    "@boomerang-io/styles": "0.0.3",
    "@boomerang-io/utils": "0.0.6",
    "@carbon/charts": "^0.16.24",
    "@carbon/charts-react": "^0.16.24",
    "@carbon/icons-react": "^10.12.0",
    "@projectstorm/react-diagrams": "^5.3.2",
    "axios": "0.19.2",
    "axios-hooks": "^2.1.0",
    "carbon-components": "10.22.0",
    "carbon-components-react": "7.22.0",
    "classnames": "^2.2.6",
    "closest": "0.0.1",
    "codemirror": "^5.48.4",
    "copy-to-clipboard": "^3.3.1",
    "cronstrue": "^1.58.0",
    "d3": "<=5.9.2",
    "detect-browser": "^4.0.3",
    "flagged": "^2.0.1",
    "formik": "^2.1.4",
    "immer": "^8.0.1",
    "js-file-download": "^0.4.4",
    "match-sorter": "^2.3.0",
    "moment-timezone": "^0.5.23",
    "pathfinding": "^0.4.18",
    "paths-js": "^0.4.10",
    "prop-types": "15.7.2",
    "query-string": "^6.8.3",
    "react": "16.13.1",
    "react-beautiful-dnd": "^9.0.2",
    "react-codemirror2": "^6.0.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "16.13.1",
    "react-dropzone": "^10.1.10",
    "react-error-boundary": "^2.3.1",
    "react-helmet": "^6.0.0",
    "react-json-view": "^1.19.1",
    "react-lazylog": "4.5.2",
    "react-markdown": "^5.0.2",
    "react-query": "2.4.14",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "reflexbox": "^4.0.6",
    "typeface-ibm-plex-sans": "0.0.75",
    "use-immer": "^0.4.0",
    "validator": "^10.7.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@cypress/webpack-preprocessor": "^5.4.2",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.2.1",
    "@testing-library/react-hooks": "^3.3.0",
    "@testing-library/user-event": "^12.0.2",
    "@types/carbon-components-react": "^7.10.6",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.9",
    "@types/lodash": "^4.14.157",
    "@types/match-sorter": "^4.0.0",
    "@types/node": "^14.0.22",
    "@types/react": "^16.9.43",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-query": "^1.1.2",
    "@types/react-router-dom": "^5.1.5",
    "@types/reflexbox": "^4.0.1",
    "@types/validator": "^13.1.0",
    "@types/yup": "^0.29.3",
    "commitizen": "4.0.3",
    "cross-env": "7.0.2",
    "cypress": "4.11.0",
    "cz-conventional-changelog": "3.1.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jest": "^23.11.0",
    "eslint-plugin-jest-dom": "3.0.1",
    "eslint-plugin-testing-library": "3.3.0",
    "find-webpack": "^2.0.0",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^4.2.5",
    "jest-axe": "^3.4.0",
    "lint-staged": "10.1.1",
    "miragejs": "^0.1.40",
    "ncp": "^2.0.0",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "react-query-devtools": "^1.2.0",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.10.11",
    "typescript": "^3.9.6",
    "typescript-plugin-css-modules": "^2.4.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,scss,css,md}": [
      "prettier --print-width 120 --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run-script lint:staged",
      "npm run-script test:staged"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!**/*.spec.{js,jsx}",
      "!**/Assets/**",
      "!**/ApiServer/**",
      "**/src/Components/**/*.{js,jsx}",
      "**/src/Features/**/*.{js,jsx}",
      "**/src/Hooks/**/*.{js,jsx}",
      "**/src/State/**/*.{js,jsx}",
      "**/src/Utils/**/*.{js,jsx}"
    ]
  }
}
